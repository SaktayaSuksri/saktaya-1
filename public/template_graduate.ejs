.card-news {
  float: left;
  /*padding: 0 1.7rem;*/
  width: 50%;
}
.card-news .menu-content {
  margin: 0;
  padding: 0;
  list-style-type: none;
}
.card-news .menu-content::before, .card-news .menu-content::after {
  content: '';
  display: table;
}
.card-news .menu-content::after {
  clear: both;
}
.card-news .menu-content li {
  display: inline-block;
}
.card-news .menu-content a {
  color: #fff;
}
.card-news .menu-content span {
  position: absolute;
  left: 50%;
  top: 0;
  font-size: 10px;
  font-weight: 700;
  font-family: 'Open Sans';
  -webkit-transform: translate(-50%, 0);
          transform: translate(-50%, 0);
}
.card-news .wrapper {
  background-color: rgba(248, 238, 211, 0.527);
  min-height: 540px;
  position: relative;
  overflow: hidden;
  box-shadow: 0 19px 38px rgba(0, 0, 0, 0.3), 0 15px 12px rgba(0, 0, 0, 0.2);
}
.card-news .wrapper:hover .data {
  -webkit-transform: translateY(0);
          transform: translateY(0);
}
.card-news .data {
  position: absolute;
  bottom: 0;
  color: #fff;
  width: 100%;
  -webkit-transform: translateY(calc(70px + 1em));
          transform: translateY(calc(70px + 1em));
  transition: -webkit-transform 0.3s;
  transition: transform 0.3s;
  transition: transform 0.3s, -webkit-transform 0.3s;
}
.card-news .data .content {
  padding: 1em;
  position: relative;
  z-index: 1;
}
.card-news .author {
  font-size: 12px;
}
.card-news .title {
  margin-top: 10px;
}
.card-news .text {
  height: 70px;
  margin: 0;
}
.card-news input[type='checkbox'] {
  display: none;
}
.card-news input[type='checkbox']:checked + .menu-content {
  -webkit-transform: translateY(-60px);
          transform: translateY(-60px);
}

